    @RenderSection("Scripts", required: false)

    @if (ViewBag.Erro != null)
    {
        <script>
            $(document).ready(function () {
                swal.fire({
                    title: "Erro!",
                    html: "@Html.Raw(ViewBag.Erro)",
                    icon: "error",
                    confirmButtonText: "Ok",
                    customClass: {
                        confirmButton: "btn font-weight-bold btn-light-primary"
                    },
                    showClass: {
                        popup: 'animate__animated animate__fadeInDown'
                    },
                    hideClass: {
                        popup: 'animate__animated animate__fadeOutUp'
                    }
                });
            });
        </script>
    }
    @if (ViewBag.Mensagem != null)
    {
        <script>
            $(document).ready(function () {
                swal.fire({
                    title: "Sucesso!",
                    html: "@Html.Raw(ViewBag.Mensagem)",
                    icon: "success",
                    confirmButtonText: "Ok",
                    customClass: {
                        confirmButton: "btn font-weight-bold btn-light-success"
                    },
                    showClass: {
                        popup: 'animate__animated animate__fadeInDown'
                    },
                    hideClass: {
                        popup: 'animate__animated animate__fadeOutUp'
                    }
                });
            });
        </script>
    }
    @if (ViewBag.MensagemVoltarLink != null)
    {
        <script>
            $(document).ready(function (){
                Swal.fire({
                    title: "Sucesso!",
                    html:  "@ViewBag.MensagemVoltarTexto",
                    icon: "success",
                    showCancelButton: true,
                    cancelButtonText: "Ok",
                    confirmButtonText: "<i class='flaticon2-back'></i>voltar",
                    showClass: {
                        popup: 'animate__animated animate__fadeInDown'
                    },
                    hideClass: {
                        popup: 'animate__animated animate__fadeOutUp'
                    },
                    customClass: {
                        confirmButton: "btn btn-primary font-weight-bolder",
                        cancelButton: "btn btn-light-dark font-weight-bolder"
                    }
                }).then(function (result) {
                if (result.value) {
                    window.location.href = "@ViewBag.MensagemVoltarLink"
                    };
                });
            });
        </script>
    }
