{
}
@section Scripts
{
    @if (User.IsInRole("Publicitario")) //Publicitario
    {
        //Home Publicitario
        <script>
        @{
            string Total = "";
            string Resta = "";
            string Consumido = "";
            string Nome = "";
            if (ViewBag.DashExibicaoCampanha != null)
            {
                Total = "[";
                Resta = "[";
                Consumido = "[";
                Nome = "['";
                @foreach (var item in ((List<DashExibicaoCampanhaModel>)ViewBag.DashExibicaoCampanha))
                {
                    Total += item.total + ",";
                    Resta += item.resta + ",";
                    Consumido += item.consumido + ",";
                    Nome += item.nome + @"','";
                }
                Total += "]";
                Resta += "]";
                Consumido += "]";
                Nome += "]";
                Total = Total.Replace(",]", "]");
                Resta = Resta.Replace(",]", "]");
                Consumido = Consumido.Replace(",]", "]");
                Nome = Nome.Replace(",']", "]");
            }
        }

        "use strict";
        // Shared Colors Definition
        const primary = '#6993FF';
        const success = '#1BC5BD';
        const info = '#8950FC';
        const warning = '#FFA800';
        const danger = '#F64E60';

        // Class definition
        var KTWidgets = function () {
            var _exibicao = function () {
                const apexChart = "#exibicao";
                var options = {
                    series: [{
                        name: 'Disponibilizadas',
                        data: @Total
                    }, {
                        name: 'A serem consumidas',
                        data: @Resta
                    }, {
                        name: 'Consumidas',
                        data: @Consumido
                    }],
                    chart: {
                        type: 'bar',
                        height: 350
                    },
                    plotOptions: {
                        bar: {
                            horizontal: false,
                            columnWidth: '55%',
                            endingShape: 'rounded'
                        },
                    },
                    dataLabels: {
                        enabled: false
                    },
                    stroke: {
                        show: true,
                        width: 2,
                        colors: ['transparent']
                    },
                    xaxis: {
                        categories: @Html.Raw(Nome),
                    },
                    yaxis: {
                        title: {
                            text: 'Horas'
                        },
                        labels: {
                            formatter: function (val, index) {
                                return val.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".");
                            }
                        }
                    },
                    fill: {
                        opacity: 1
                    },
                    tooltip: {
                        y: {
                            formatter: function (val) {
                                return val.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ".") + " horas"
                            }
                        }
                    },
                    colors: [primary, success, warning]
                };

                var chart = new ApexCharts(document.querySelector(apexChart), options);
                chart.render();
            }
            return {
                init: function () {
                    _exibicao();
                }
            }
        }();

        // Webpack support
        if (typeof module !== 'undefined') {
            module.exports = KTWidgets;
        }

        jQuery(document).ready(function () {
            KTWidgets.init();
        });

        </script>
    }
}
