                                <div class="symbol symbol-40 symbol-light-white mr-5">
                                    <div class="symbol-label">
                                        @if (String.IsNullOrEmpty(item.avatar))
                                        {
                                            @switch (item.idSexo)
                                            {
                                                case 1:
                                                    <img src="@Url.Content("~/assets/media/svg/avatars/001-boy.svg")" class="h-75 align-self-end" alt="" />
                                                    break;
                                                case 2:
                                                    <img src="@Url.Content("~/assets/media/svg/avatars/002-girl.svg")" class="h-75 align-self-end" alt="" />
                                                    break;
                                                default:
                                                    <img src="@Url.Content("~/assets/media/svg/avatars/035-boy-15.svg")" class="h-75 align-self-end" alt="" />
                                                    break;
                                            }
                                        }
                                        else
                                        {
                                            <img src="@(Url.Content(Local.Configuracao("URL_CDN") + "avatar/" +  item.avatar))" class="h-75" alt="" />
